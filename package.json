{
  "name": "nyerat-hugo-theme",
  "version": "0.2.4",
  "private": true,
  "description": "Fast, simple & clean blog theme for Hugo.",
  "keywords": [
    "hugo",
    "nyerat",
    "tailwindcss"
  ],
  "homepage": "https://github.com/datakrama/hugo-themes/tree/main/nyerat#readme",
  "bugs": {
    "url": "https://github.com/datakrama/hugo-themes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datakrama/hugo-themes.git"
  },
  "license": "MIT",
  "author": {
    "name": "Ahmad Husen",
    "email": "hi@husen.id",
    "url": "https://husen.id"
  },
  "engines": {
    "node": ">=14.x.x"
  },
  "scripts": {
    "dev": "HUGO_THEME_ENVIRONMENT=development run-p dev:*",
    "dev:hugo": "hugo server -s exampleSite --themesDir=../.. --disableFastRender",
    "dev:cms": "netlify-cms-proxy-server",
    "build": "HUGO_THEME_ENVIRONMENT=production hugo -s exampleSite --themesDir=../.. --gc --minify",
    "format": "run-s format:*",
    "format:eslint": "eslint --ext .js ./ --fix",
    "format:prettier": "prettier --write .",
    "prebuilt": "run-s prebuilt:*",
    "prebuilt:hugo": "HUGO_THEME_ENVIRONMENT=prebuilt hugo -s exampleSite --themesDir=../.. --gc",
    "prebuilt:js": "cp -f exampleSite/public/js/netlify-*.min.js assets/js",
    "prebuilt:css": "cp -f exampleSite/public/css/main*.min.css assets/css"
  },
  "devDependencies": {
    "@netlify/plugin-lighthouse": "^2.1.2",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.3.1",
    "browserslist": "^4.16.7",
    "cssnano": "^5.0.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "netlify-cms-app": "^2.15.40",
    "netlify-cms-proxy-server": "^1.3.20",
    "netlify-identity-widget": "^1.9.2",
    "netlify-plugin-hugo-cache-resources": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2",
    "prettier-plugin-go-template": "^0.0.11",
    "prettier-plugin-tailwind": "^2.2.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sort-package-json": "^1.50.0",
    "tailwindcss": "^2.2.7",
    "tailwindcss-interaction-variants": "^5.0.0",
    "tailwindcss-pseudo-elements": "^2.0.0"
  }
}
